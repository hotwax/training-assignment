//json input
{
  "students": [
    {
      "name": "John Doe",
      "age": 23,
      "email": "john.doe@example.com"
    },
    {
      "name": "Jane Smith",
      "age": 25,
      "email": "jane.smith@example.com"
    }
  ]
}

//json output
[  {   
        "Name": "John Doe",   
        "Age": 23,  
        "Domain": "example.com" 
    },  
    {   
        "Name": "Jane Smith", 
        "Age": 25,   
        "Domain": "example.com" 
    }
]

//json spec
[
  {
    "operation": "shift",
    "spec": {
      "students": {
        "*": {
          "name": "[#2].Name",
          "age": "[#2].Age",
          "email": "[#2].email"
        }
       }
      }
     },
  {
    "operation": "modify-overwrite-beta",
    "spec": {
      "*": {
        "domain": "=split('@',@(1,email))",
        "Domain": "=lastElement(@(1,domain))"
      }
    }
   },
  {
    "operation": "remove",
    "spec": {
      "*": {
        "domain": "",
        "email": ""
      }
    }
  }
 ]
