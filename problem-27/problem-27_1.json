        "Return Amount|Return Total Amount": "[&1].&",
        "country": {
          "USA": {
            "#United States of America": "[&3].Country"
          },
          "CA": {
            "#Canada": "[&3].Country"
          },
          "": {
            "#NA": "[&3].Country"
          }
        }
      }
    }
