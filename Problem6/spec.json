[
  {
    "operation": "modify-overwrite-beta",
    "spec": {
      "users": {
        "*": {
          "address": "=concat(@(0,street),', ',@(0,city),', ',@(0,state),' ',@(0,zip))"
        }
      }
    }
  }, {
    "operation": "shift",
    "spec": {
      "users": {
        "*": {
          "name": "[&1].Name",
          "address": "[&1].Address",
          "hobbies": {
            "*": {
              "name": "[#4].Hobbies"
            }
          }
        }
      }
    }
  }
]