// Input Json

{
  "name": "John",
  "age": 35,
  "city": "New York",
  "phone": "+1-555-123-4567"
}

// Output Json

{
  "Name": "John",
  "Age": 35,
  "Contact": {
    "City": "New York",
    "Phone": "+1-555-123-4567"
  }
}

// Jolt Spec

[
  {
    "operation": "shift",
    "spec": {
      "name": "Name",
      "age": "Age",
      "city": "Contact.City",
      "phone": "Contact.Phone"
    }
  }
]

// Used dot operator to Change the Level of city and phone
