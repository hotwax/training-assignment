[
    {
      "operation": "modify-overwrite-beta",
      "spec": {
        "items": {
          "*": {
            "prices": {
              "*": {
                "currency": "=concat(@(3,id),'_',@(1,currency))"
              }
            }
          }
        }
      }
    },
    {
      "operation": "shift",
      "spec": {
        "items": {
          "*": {
            "prices": {
              "*": {
                "@(value)": "@(1,currency)"
              }
            }
          }
        }
      }
    }
  ]