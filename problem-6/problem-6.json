[
  {
    "operation": "modify-overwrite-beta",
    "spec": {
      "users": {
        "*": {
          "address": "=concat(@(0,street), ', ', @(0,city), ', ', @(0,state), ' ' , @(0,zip))"
        }
      }
    }
  },
  {
    "operation": "shift",
    "spec": {
      "users": {
        "*": {
          "address": "[&1].Address",
          "name": "[&1].Name",
          "hobbies": {
            "*": {
              "name": "[&3].Hobbies"
            }
          }
        }
      }
    }
  }
]