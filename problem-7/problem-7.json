[
  {
    "operation": "modify-overwrite-beta",
    "spec": {
      "students": {
        "*": {
          "email": "=split('@', @(1,email))",
          "Domain": "@(1,email[1])"
        }
      }
    }
  },
  {
    "operation": "shift",
    "spec": {
      "students": {
        "*": {
          "name": "[&1].Name",
          "age": "[&1].Age",
          "Domain": "[&1].&"
        }
      }
    }
  }
]